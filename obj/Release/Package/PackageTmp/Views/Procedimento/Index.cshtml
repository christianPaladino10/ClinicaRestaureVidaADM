@model IEnumerable<AdminRestaureVida.Models.Procedimento>

@{
    ViewBag.Title = "Index";
}

    <h2>Técnicas</h2>

<p>
    @Html.ActionLink("Cadastrar Nova Técnica", "Create")
</p>
<table class="table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            <a href="/Procedimento/Edit/@item.Id" data-toggle="tooltip" data-placement="top" title="Editar"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a>
            @*<a href="/Procedimento/Details/@item.Id" data-toggle="tooltip" data-placement="top" title="Ver Detalhes"><span class="glyphicon glyphicon-eye-open" aria-hidden="true" style="color:black"></span></a>*@
        </td>
    </tr>
}

</table>
